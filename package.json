{
  "name": "@microsoft/fluentui-eslint-plugin-jsx-a11y",
  "version": "1.0.0",
  "description": "Static AST checker for accessibility rules on FluentUI JSX elements.",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "a11y",
    "accessibility",
    "jsx",
    "fluentui",
    "javascript",
    "typescript",
    "lint"
  ],
  "author": "Aubrey Quinn",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/microsoft/fluentui-eslint-plugin-jsx-a11y.git"
  },
  "files": [
    "lib"
  ],
  "main": "./lib/index.js",
  "exports": "./lib/index.js",
  "scripts": {
    "lint": "npm-run-all \"lint:*\"",
    "lint:eslint-docs": "npm-run-all \"update:eslint-docs -- --check\"",
